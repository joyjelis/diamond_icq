<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::stores">
                    <resource id="Magepow_OnestepCheckout::onestepcheckout" title="One Step Checkout" sortOrder="45">
                        <resource id="Magepow_OnestepCheckout::field_management" title="Manage Fields" sortOrder="10"/>
                    </resource>
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Magepow_OnestepCheckout::configuration" title="Magepow One Step Checkout"/>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>

               var error = false;
               var ring_size =  $("#ring_size").val();
               if (!ring_size) {
                  error = true;
                  $(".ring-size-error").show();
               } else {
                  $(".ring-size-error").hide();
               }
               return !error;
            });
         });
     })
</script>
